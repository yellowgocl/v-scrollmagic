{
  "name": "v-scrollmagic",
  "version": "0.0.2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "bundle": "vue-cli-service build --target lib --name v-scrollmagic ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/v-scrollmagic.common.js",
  "files": [
    "dist/*",
    "src/components/*",
    "src/assets/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "gsap": "^2.0.2",
    "lodash": "^4.17.4",
    "scrollmagic": "^2.0.6",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "allowIndentationTabs": true,
      "indent": [
        "error",
        4
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
